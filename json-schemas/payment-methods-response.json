{
	"$id": "https://github.com/wp-pay-gateways/adyen/blob/master/json-schemas/payment-methods-response.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Adyen payment methods response.",
	"type": "object",
	"properties": {
		"groups": {
			"type": "array",
			"description": "Groups of payment methods."
		},
		"oneClickPaymentMethods": {
			"type": "array",
			"description": "Detailed list of one-click payment methods"
		},
		"paymentMethods": {
			"type": "array",
			"description": "Detailed list of payment methods required to generate payment forms."
		}
	},
	"required": [
		"groups",
		"paymentMethods"
	]
}
